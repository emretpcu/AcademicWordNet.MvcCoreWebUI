@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge"> 
	<!-- Tell the browser to be responsive to screen width -->
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!-- Font Awesome -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">
	<!-- Ionicons -->
	<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
	<!-- Tempusdominus Bbootstrap 4 -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
	<!-- iCheck -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
	<!-- JQVMap -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/jqvmap/jqvmap.min.css">
	<!-- Theme style -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/dist/css/adminlte.min.css">
	<!-- overlayScrollbars -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
	<!-- Daterange picker -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/daterangepicker/daterangepicker.css">
	<!-- summernote -->
	<link rel="stylesheet" href="/Tema Panel/AdminLTE-3.0.4/plugins/summernote/summernote-bs4.css">
	<!-- Google Font: Source Sans Pro -->
	<link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body>
	@*--Main Nav Start--*@
	<!-- Main Sidebar Container -->
	<aside class="main-sidebar sidebar-dark-primary elevation-4">
		<!-- Brand Logo -->
		<a href="/home" class="brand-link">
			<img src="/Tema Panel/AdminLTE-3.0.4/dist/img/AdminLTELogo.png" alt="Tatli Sozluk Logo" class="brand-image img-circle elevation-3"
				 style="opacity: .8">
			<span class="brand-text font-weight-light">tatlisözluk</span>
		</a>

		<!-- Sidebar -->
		<div class="sidebar">
			<!-- Sidebar user panel (optional) -->
			<div class="user-panel mt-3 pb-3 mb-3 d-flex">
				<div class="image">
					<img src="/Tema Panel/AdminLTE-3.0.4/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
				</div>
				<div class="info">
					<a href="/admin" class="d-block">@User.Identity.Name</a>
				</div>
			</div>

			<!-- Sidebar Menu -->
			<nav class="mt-2">
				<ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
					<!-- Add icons to the links using the .nav-icon class
						 with font-awesome or any other icon font library -->
					<li class="nav-item">
						<a href="/admin/" class="nav-link @*active*@">
							<i class="nav-icon fas fa-tachometer-alt"></i>
							<p>Gösterge Paneli</p>
						</a>
					</li>
					<li class="nav-item">
					</li>
					<li class="nav-item">
						<a href="/admin/editcategory" class="nav-link @*active*@">
							<i class="nav-icon fas fa-edit"></i>
							<p>Kategorileri Düzenle</p>
						</a>
					</li>
					<li class="nav-item has-treeview">
						<a href="" class="nav-link">
							<i class="nav-icon fas fa-chart-pie"></i>
							<p>
								Kötü Sözcük
								<i class="right fas fa-angle-left"></i>
							</p>
						</a>
						<ul class="nav nav-treeview">
							<li class="nav-item">
								<a href="/admin/badwordsreports/" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Kötü Sözcük Raporları</p>
								</a>
							</li>
							<li class="nav-item">
								<a href="/admin/editbadwords" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Kötü Sözcük Ekle Kaldır</p>
								</a>
							</li>
						</ul>
					</li>
					<li class="nav-item has-treeview">
						<a href="#" class="nav-link">
							<i class="nav-icon far fa-envelope"></i>
							<p>
								Başlık Yönetimi
								<i class="fas fa-angle-left right"></i>
							</p>
						</a>
						<ul class="nav nav-treeview">
							<li class="nav-item">
								<a href="/admin/pendingheadingapprovals" class="nav-link @*active*@">
									<i class="far fa-circle nav-icon"></i>
									<p>Onay Bekleyen</p>
								</a>
							</li>
							<li class="nav-item">
								<a href="/admin/deactivatedheadings" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Devre Dışı Bırakılmış</p>
								</a>
							</li>
						</ul>
					</li>

					<li class="nav-item has-treeview">
						<a href="#" class="nav-link">
							<i class="nav-icon fas fa-book"></i>
							<p>
								Entry Yönetimi
								<i class="fas fa-angle-left right"></i>
							</p>
						</a>
						<ul class="nav nav-treeview">
							<li class="nav-item">
								<a href="/admin/deactivatedentries" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Devre Dışı Bırakılmış</p>
								</a>
							</li>
						</ul>
					</li>
					<li class="nav-item has-treeview">
						<a href="#" class="nav-link">
							<i class="nav-icon fas fa-tree"></i>
							<p>
								Admin İşlemleri
								<i class="fas fa-angle-left right"></i>
							</p>
						</a>
						<ul class="nav nav-treeview">
							<li class="nav-item">
								<a href="/admin/assignadmin" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Admin Ata</p>
								</a>
							</li>
							<li class="nav-item">
								<a href="/admin/getchats" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Mesajları Görüntüle</p>
								</a>
							</li>
							<li class="nav-item">
								<a href="/admin/getstudentsinformations" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Sınıf İşlemleri</p>
								</a>
							</li>
							<li class="nav-item">
								<a href="/admin/getstudentsaccount" class="nav-link">
									<i class="far fa-circle nav-icon"></i>
									<p>Hesapları Görüntüle</p>
								</a>
							</li>
						</ul>
					</li>
					
				</ul>
			</nav>
			<!-- /.sidebar-menu -->
		</div>
		<!-- /.sidebar -->
	</aside>
	@*--Main Nav End--*@
	@*--Content Start--*@
	<div class="content-wrapper">@RenderBody()</div>
	@*--Content End--*@
	<!-- jQuery -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
	<!-- jQuery UI 1.11.4 -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/jquery-ui/jquery-ui.min.js"></script>
	<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
	<script>
		$.widget.bridge('uibutton', $.ui.button)
	</script>
	<!-- Bootstrap 4 -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
	<!-- ChartJS -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/chart.js/Chart.min.js"></script>
	<!-- Sparkline -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/sparklines/sparkline.js"></script>
	<!-- JQVMap -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/jqvmap/jquery.vmap.min.js"></script>
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
	<!-- jQuery Knob Chart -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/jquery-knob/jquery.knob.min.js"></script>
	<!-- daterangepicker -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/moment/moment.min.js"></script>
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/daterangepicker/daterangepicker.js"></script>
	<!-- Tempusdominus Bootstrap 4 -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
	<!-- Summernote -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/summernote/summernote-bs4.min.js"></script>
	<!-- overlayScrollbars -->
	<script src="/Tema Panel/AdminLTE-3.0.4/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
	<!-- AdminLTE App -->
	<script src="/Tema Panel/AdminLTE-3.0.4/dist/js/adminlte.js"></script>
	<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
	<script src="/Tema Panel/AdminLTE-3.0.4/dist/js/pages/dashboard.js"></script>
	<!-- AdminLTE for demo purposes -->
	<script src="/Tema Panel/AdminLTE-3.0.4/dist/js/demo.js"></script>
</body>
</html>
